{
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "backebnd",
    "main": "server.ts",
    "keywords": [],
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register --transpile-only src/server.ts",
        "dev": "nodemon",
        "production":"export NODE_ENV=test || SET \"NODE_ENV=test\" && nodemon",
        "lint": "eslint --ignore-path .gitignore --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "migration:generate": "sequelize migration:generate --name",
        "migration": "sequelize db:migrate",
        "seed:generate": "npx sequelize-cli seed:generate --name",
        "seed:all": "npx sequelize-cli db:seed:all"
    },
    "author": "Murad",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.5",
        "sequelize-typescript": "^2.1.6",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tsconfig-paths": "^4.2.0",
        "winston": "^3.15.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^22.7.6",
        "@types/pg": "^8.11.10",
        "@types/sequelize": "^4.28.20",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^3.1.7",
        "prettier": "^2.7.1",
        "sequelize-cli": "^6.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^4.7.4"
    }
}
